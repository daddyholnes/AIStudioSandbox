{
  "name": "aistudiosandbox",
  "version": "1.0.0",
  "type": "module",
  "description": "AI Studio Sandbox for testing and development",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "tsx watch server/index.ts",
    "client": "vite",
    "build": "tsc && vite build",
    "start": "node server/index.ts",
    "websocket": "tsx server/websocket.ts",
    "test": "node test-genkit.js"
  },
  "dependencies": {
    "@esbuild/win32-x64": "0.19.2",
    "@genkit-ai/core": "^1.5.0",
    "@genkit-ai/googleai": "^1.5.0",
    "express": "^4.18.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "tsx": "^4.7.1",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
